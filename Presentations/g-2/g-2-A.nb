Cell[BoxData[{
 RowBox[{
  RowBox[{
   RowBox[{"MakeBoxes", "[", 
    RowBox[{"mu", ",", "TraditionalForm"}], "]"}], ":=", "\"\<\[Mu]\>\""}], 
  ";"}], "\n", 
 RowBox[{
  RowBox[{
   RowBox[{"MakeBoxes", "[", 
    RowBox[{"p1", ",", "TraditionalForm"}], "]"}], ":=", 
   "\"\<\\!\\(\\*SubscriptBox[\\(p\\), \\(1\\)]\\)\>\""}], ";"}], "\n", 
 RowBox[{
  RowBox[{
   RowBox[{"MakeBoxes", "[", 
    RowBox[{"p2", ",", "TraditionalForm"}], "]"}], ":=", 
   "\"\<\\!\\(\\*SubscriptBox[\\(p\\), \\(2\\)]\\)\>\""}], ";"}], "\n", 
 RowBox[{
  RowBox[{
   RowBox[{"diags", "=", 
    RowBox[{"InsertFields", "[", 
     RowBox[{
      RowBox[{"CreateTopologies", "[", 
       RowBox[{"1", ",", 
        RowBox[{"1", "->", "2"}], ",", 
        RowBox[{"ExcludeTopologies", "->", 
         RowBox[{"{", 
          RowBox[{"Tadpoles", ",", "WFCorrections"}], "}"}]}]}], "]"}], ",", 
      RowBox[{
       RowBox[{"{", 
        RowBox[{"F", "[", 
         RowBox[{"2", ",", 
          RowBox[{"{", "2", "}"}]}], "]"}], "}"}], "->", 
       RowBox[{"{", 
        RowBox[{
         RowBox[{"V", "[", "1", "]"}], ",", 
         RowBox[{"F", "[", 
          RowBox[{"2", ",", 
           RowBox[{"{", "2", "}"}]}], "]"}]}], "}"}]}], ",", 
      RowBox[{"InsertionLevel", "->", 
       RowBox[{"{", "Classes", "}"}]}], ",", 
      RowBox[{"ExcludeParticles", "->", 
       RowBox[{"{", 
        RowBox[{
         RowBox[{"S", "[", "1", "]"}], ",", 
         RowBox[{"S", "[", "2", "]"}], ",", 
         RowBox[{"S", "[", "3", "]"}], ",", 
         RowBox[{"V", "[", "2", "]"}], ",", 
         RowBox[{"V", "[", "3", "]"}]}], "}"}]}]}], "]"}]}], ";"}], 
  "\[IndentingNewLine]", 
  RowBox[{"(*", 
   RowBox[{
    RowBox[{"diagsCT", "=", 
     RowBox[{"InsertFields", "[", 
      RowBox[{
       RowBox[{"CreateCTTopologies", "[", 
        RowBox[{"1", ",", 
         RowBox[{"1", "->", "2"}], ",", 
         RowBox[{"ExcludeTopologies", "->", 
          RowBox[{"{", 
           RowBox[{"Tadpoles", ",", "WFCorrections"}], "}"}]}]}], "]"}], ",", 
       
       RowBox[{
        RowBox[{"{", 
         RowBox[{"F", "[", 
          RowBox[{"2", ",", 
           RowBox[{"{", "1", "}"}]}], "]"}], "}"}], "->", 
        RowBox[{"{", 
         RowBox[{
          RowBox[{"V", "[", "1", "]"}], ",", 
          RowBox[{"F", "[", 
           RowBox[{"2", ",", 
            RowBox[{"{", "1", "}"}]}], "]"}]}], "}"}]}], ",", 
       RowBox[{"InsertionLevel", "->", 
        RowBox[{"{", "Particles", "}"}]}], ",", 
       RowBox[{"ExcludeParticles", "->", 
        RowBox[{"{", 
         RowBox[{
          RowBox[{"S", "[", "1", "]"}], ",", 
          RowBox[{"S", "[", "2", "]"}], ",", 
          RowBox[{"S", "[", "3", "]"}], ",", 
          RowBox[{"V", "[", "2", "]"}], ",", 
          RowBox[{"V", "[", "3", "]"}]}], "}"}]}]}], "]"}]}], ";"}], "*)"}], 
  "\[IndentingNewLine]"}], "\n", 
 RowBox[{
  RowBox[{"Paint", "[", 
   RowBox[{"diags", ",", 
    RowBox[{"ColumnsXRows", "->", 
     RowBox[{"{", 
      RowBox[{"1", ",", "1"}], "}"}]}], ",", 
    RowBox[{"Numbering", "->", "Simple"}], ",", 
    RowBox[{"SheetHeader", "->", "None"}], ",", 
    RowBox[{"ImageSize", "->", 
